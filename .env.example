# Example environment file for local development
# Copy this file to `.env` and fill in real values. Do NOT commit the resulting `.env` file.

# PostgreSQL connection string
PG_URL=postgresql://postgres:password@localhost:5432/mydb

# Git / Sync configuration
GIT_REMOTE=origin
GIT_BRANCH=main

# API keys and tokens (placeholders)
GITHUB_PERSONAL_ACCESS_TOKEN=REDACTED
SENTRY_AUTH_TOKEN=REDACTED_SENTRY_TOKEN
NOTION_TOKEN=REDACTED
MEM0_API_KEY=REDACTED_MEM0_API_KEY
CONTEXT7_API_KEY=REDACTED_CONTEXT7_API_KEY

# Firebase example placeholders
VITE_FIREBASE_API_KEY=REDACTED_FIREBASE_API_KEY
VITE_FIREBASE_AUTH_DOMAIN=your-firebase-app.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id

# Server-specific secrets
MCP_FS_API_KEY=REDACTED_MCP_FS_API_KEY
CANVA_MCP_ACCESS_TOKEN=REDACTED

# NOTE: Add any local-only secrets to your local .env, which is ignored by git by default.
